#include <iostream>
using namespace std;

void fill_matrix(int** mat, int rows, int cols) {
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            mat[i][j] = rand() % 10;
        }
    }
}

int** multiply_matrix(int** A, int** B, int n, int m, int k) {
    int** C = new int*[n];
    for (int i = 0; i < n; i++) {
        C[i] = new int[k];
        for (int j = 0; j < k; j++) {
            C[i][j] = 0;
            for (int l = 0; l < m; l++) {
                C[i][j] += A[i][l] * B[l][j];
            }
        }
    }
    return C;
}

void print_matrix(int** mat, int rows, int cols) {
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            cout << mat[i][j] << " ";
        }
        cout << endl;
    }
}

int main() {
    int n = rand() % 10;
    int m = rand() % 10;
    int k = rand() % 10;

    int** A = new int*[n];
    for (int i = 0; i < n; i++)
        A[i] = new int[m];

    int** B = new int*[m];
    for (int i = 0; i < m; i++)
        B[i] = new int[k];

    fill_matrix(A, n, m);
    fill_matrix(B, m, k);

    int** C = multiply_matrix(A, B, n, m, k);

    cout << "A:\n";
    print_matrix(A, n, m);
    cout << "B:\n";
    print_matrix(B, m, k);
    cout << "C = A * B:\n";
    print_matrix(C, n, k);
    return 0;
}
